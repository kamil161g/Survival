{% extends 'base.html.twig' %}
    {% block body %}

        <h1 class="btn btn-default btn-lg btn-block" style=" width: 30%">Wioska : {{ village.name }}</h1>
        <a href="{{ path('viewFood') }}">
            <h3 class="btn btn-primary btn-lg btn-block" style=" width: 30%">Jedzenie : {{ food.value }}</h3>
        </a>
        <a href="{{ path('viewStone') }}">
            <h3 class="btn btn-primary btn-lg btn-block" style=" width: 30%">Kamień : {{ stone.value }}</h3>
        </a>
        <a href="{{ path('viewCarbon') }}">
            <h3 class="btn btn-primary btn-lg btn-block" style=" width: 30%">Węgiel : {{ carbon.value }}</h3>
        </a>
        <a href="{{ path('viewGold') }}">
            <h3 class="btn btn-primary btn-lg btn-block" style=" width: 30%">Złoto : {{ gold.value }}</h3>
        </a>
        <a href="{{ path('viewWood') }}">
            <h3 class="btn btn-primary btn-lg btn-block" style=" width: 30%">Drewno : {{ wood.value }}</h3>
        </a>
        <h3 class="btn btn-primary btn-lg btn-block" style=" width: 30%">Poziom wioski : {{ village.level }}</h3>
        <h3 class="btn btn-success btn-lg btn-block" style=" width: 30%">Populacja wioski : {{ village.population }}</h3>



    {% endblock %}